{
  "name": "express-microservice-template",
  "version": "1.0.0",
  "description": "A production-grade template for Express.js microservices generated by AI.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watchAll",
    "test:e2e": "npx playwright test",
    "test:e2e:report": "npx playwright show-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:build": "tsc --noEmit -p tsconfig.build.json",
    "dev:safe": "npm run type-check && npm run dev",
    "build:safe": "npm run type-check:build && npm run build",
    "seed": "ts-node scripts/seed-database.ts",
    "generate:test-data": "ts-node scripts/generate-test-data.ts",
    "generate:component": "ts-node scripts/generate-component.ts",
    "generate:test": "ts-node scripts/generate-test.ts",
    "setup:dev": "ts-node scripts/setup-dev.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "health": "curl -s http://localhost:4010/ | jq .",
    "docs": "echo 'API Documentation available at http://localhost:4010/api-docs'",
    "clean": "rm -rf dist test-results playwright-report test-data",
    "reset": "npm run clean && npm install && npm run seed",
    "prepare": "husky"
  },
  "keywords": [
    "express",
    "typescript",
    "microservice",
    "nodejs",
    "template"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@prisma/client": "^6.13.0",
    "@types/axios": "^0.9.36",
    "@types/passport": "^1.0.17",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.1",
    "@types/speakeasy": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.13.0",
    "qrcode": "^1.5.4",
    "redis": "^5.6.1",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.17.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.4",
    "@types/pino": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
